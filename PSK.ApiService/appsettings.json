{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AuditLog": {
    "Enabled": true,
    "LoggerType": "MongoDB",
    "ExcludedPaths": ["/swagger"]
  },
  "MongoDB": {
    "ConnectionString": "{mongodb.connectionString}",
    "DatabaseName": "AuditLogs"
  }
}
